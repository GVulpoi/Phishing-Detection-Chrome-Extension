{
	"manifest_version": 3,
	"name": "Phishing Detector",
	"description": "Detects phishing activities in websites.",
	"version": "1.0",
	"permissions": ["scripting", "activeTab", "storage", "alarms", "declarativeNetRequest", "management", "tabs"],
	"host_permissions": ["<all_urls>"],
	"action": {
		"default_popup": "./Popup/popup.html",
		"default_icon": {
			"16": "./Resources/icon-16.png",
			"32": "./Resources/icon-32.png"
		}
	},
	"icons": {
	"16": "./Resources/icon-16.png",
	"32": "./Resources/icon-32.png",
	"48": "./Resources/icon-48.png",
	"128": "./Resources/icon-128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"exclude_matches": [
				"https://www.google.com/*"
			],
			"js": ["Scripts/PredictionLogic.js"],
			"type": "module",
			"run_at": "document_idle"
		}
	],
	"background": {
		"service_worker": "ServiceWorker.js",
		"type": "module"
	},
	"web_accessible_resources": [
	{
		"resources": ["Model/model.json", "Model/group1-shard1of1.bin"],
		"matches": ["<all_urls>"]
	}
]
}
